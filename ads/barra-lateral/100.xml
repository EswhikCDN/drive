document.write(`
<style>
/* Fixed Ad Slot */

.fixed-leftSd,
.fixed-rightSd {
    position: fixed;
    top: 60px;
    width: 160px;
    height: 600px;
    z-index: 9999;
    transform: translateZ(0)
}
.fixed-leftSd {
    left: 0
}
.fixed-rightSd {
    right: 0
}
.close-fixedSd {
    position: absolute;
    width: 160px;
    height: 25px;
    line-height: 15px;
    font-size: 11px;
    font-weight: 400;
    top: -25px;
    left: 0;
    text-align: center;
    background: rgba(0, 123, 255,0.9);
    color: #ffffff;
    padding: 5px 0;
    cursor: pointer
}
.adline {
    display: block;
    line-height: normal;
    font-weight: 700;
    background: rgba(0, 0, 0,0.6);
    overflow: hidden;
    width: 100%;
    max-width: 160px;
    height: 600px;
    text-align: center;
    color: #fff
}
.adlines {
    display: table;
    font-weight: 400;
    line-height: 600px;
    text-align: center;
    color: #fff;
    margin: auto
}
@media screen and (max-width: 800px) {
    .fixed-leftSd,
    .fixed-rightSd {
        display: none;
        visibility: hidden;
    }
}
</style>
`)